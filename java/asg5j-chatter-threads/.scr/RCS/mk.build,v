@#!/bin/sh
# $Id: mk.build,v 1.1 2009-03-06 21:24:34-08 - - $
# Checksource and do the build.
PATH=$PATH:/afs/cats.ucsc.edu/courses/cmps012b-wm/bin/
export PATH
checksource M* R* *.java >checksource.log 2>&1
gmake >gmake.log 2>&1
@
